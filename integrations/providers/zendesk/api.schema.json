      "name": "Create Contact in Zendesk",
      "method": "POST",
      "url": "https://{{subdomain}}.zendesk.com/api/v2/users",
      "headers": [
        {
          "key": "Authorization",
          "value": "Bearer {{auth_token}}"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Accept",
          "value": "application/json"
        }
      ],
      "queryParams": [],
      "bodyType": "json",
      "body": {
        "json": {
          "user": {
            "email": "{{email}}",
            "name": "{{contact_data}}",
            "role": "end-user",
            "verified": true,
            "custom_fields": {
              "source": "{{source}}",
              "auto_assign": "{{auto_assign}}",
              "owner": "{{default_owner}}"
            }
          }
        }
      },
      "response": {
        "successPath": "user.id",
        "errorPath": "error.message",
